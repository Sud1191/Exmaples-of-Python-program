n, i = 1, 10
while i >=n:
    print(i)
    i= i-1
----------------------------------------
 # Write a Python program to iterate over dictionaries using for loop
d = {'x': 10, 'y': 20, 'z': 30} 
print(d.items())
for dict_key, dict_value in d.items():
    print(dict_key,'->',dict_value)
    
for i,j in d.items():
    print(i,":",j)
---------------------------------------
l = [1,4,7,8,5,2,3,6,9]
print(l[-1::-1])
==>
[9, 6, 3, 2, 5, 8, 7, 4, 1]
---------------------------------------
